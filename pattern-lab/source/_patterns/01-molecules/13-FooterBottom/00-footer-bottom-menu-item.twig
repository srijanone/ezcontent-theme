{% for key, value in footermenu.footerBottom %}
  <li {{ value.attributes }}>
    {% include '@atoms/12-links/00-links.twig' with {
      text: value.text,
      class: value.class,
      url: value.url
    } %}
  </li>
{% endfor %}
<p class="copyright-info">&copy; {{ footermenu.copy_right.date }} {{ footermenu.copy_right.sitename }} {{ footermenu.copy_right.text }}</p>
