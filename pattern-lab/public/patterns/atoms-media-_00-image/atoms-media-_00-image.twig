{#
/**
 * @file
 * Theme override of an image.
 *
 * Available variables:
 * - attributes: HTML attributes for the img tag.
 * - style_name: (optional) The name of the image style applied.
 *
 * @see template_preprocess_image()
 */
#}
<img
  {% if image_srcset %}
  srcset="{{ image_srcset }}"
  {% endif %}
  {% if image_sizes %}
    sizes="{{ image_sizes }}"
  {% endif %}
  src="{{ image_src }}"
  {% if image_alt %}
    alt="{{ image_alt }}"
  {% endif %}
  {% if image_title %}
    title="{{ image_title }}"
  {% endif %} 
/>
